## SECTION=wago_specific_libs
		
config LIBDPS
	bool
	default n
	select LIBOSAL
	select LIBUTIL
	select DAL
	select LIBDPC31STACK
	select LIBDPC31DRIVER
	select LIBEXTMEMCPY
	select LED_SERVER_META
	select WAGO_PFC_DIAGNOSTIC
